			clocks = <0x3 0x2f 0x3 0x70 0x3 0x6f>;
			clock-names = "iahb", "isfr", "tmds";
			resets = <0x3 0x21>;
			reset-names = "ctrl";
			phys = <0x21>;
			phy-names = "hdmi-phy";
			status = "okay";

			ports {
				#address-cells = <0x1>;
				#size-cells = <0x0>;

				port@0 {
					reg = <0x0>;

					endpoint {
						remote-endpoint = <0x22>;
						phandle = <0x7>;
					};
				};

				port@1 {
					reg = <0x1>;

					endpoint {
						remote-endpoint = <0x23>;
						phandle = <0x29>;
					};
				};
			};
		};

		hdmi-phy@1ef0000 {
			compatible = "allwinner,sunh5_hdmi_phy";
			reg = <0x1ef0000 0x10000>;
			clocks = <0x3 0x2f 0x3 0x70 0x3 0x6>;
			clock-names = "bus", "mod", "pll-0";
			resets = <0x3 0x20>;
			reset-names = "phy";
			#phy-cells = <0x0>;
			status = "okay";
			phandle = <0x21>;
		};

		rtc@1f00000 {
			reg = <0x1f00000 0x400>;
			interrupts = <0x0 0x28 0x4 0x0 0x29 0x4>;
			clock-output-names = "osc32k", "osc32k-out", "iosc";
			clocks = <0x24>;
			#clock-cells = <0x1>;
			compatible = "allwinner,sun50i-h5-rtc";
			phandle = <0xf>;
		};

		clock@1f01400 {
			compatible = "allwinner,sun8i-h3-r-ccu";
			reg = <0x1f01400 0x100>;
			clocks = <0xe 0xf 0x0 0xf 0x2 0x3 0x9>;
			clock-names = "hosc", "losc", "iosc", "pll-periph";
			#clock-cells = <0x1>;
			#reset-cells = <0x1>;
			phandle = <0x25>;
		};

		codec-analog@1f015c0 {
			compatible = "allwinner,sun8i-h3-codec-analog";
			reg = <0x1f015c0 0x4>;
			phandle = <0x19>;
		};

		ir@1f02000 {
			compatible = "allwinner,sun5i-a13-ir";
			clocks = <0x25 0x4 0x25 0xb>;
			clock-names = "apb", "ir";
			resets = <0x25 0x0>;
			interrupts = <0x0 0x25 0x4>;
			reg = <0x1f02000 0x400>;
			status = "disabled";
		};

		i2c@1f02400 {
			compatible = "allwinner,sun6i-a31-i2c";
			reg = <0x1f02400 0x400>;
			interrupts = <0x0 0x2c 0x4>;
			pinctrl-names = "default";
			pinctrl-0 = <0x26>;
			clocks = <0x25 0x9>;
			resets = <0x25 0x5>;
			status = "disabled";
			#address-cells = <0x1>;
			#size-cells = <0x0>;
		};

		pinctrl@1f02c00 {
			compatible = "allwinner,sun8i-h3-r-pinctrl";
			reg = <0x1f02c00 0x400>;
			interrupts = <0x0 0x2d 0x4>;
			clocks = <0x25 0x3 0xe 0xf 0x0>;
			clock-names = "apb", "hosc", "losc";
			gpio-controller;
			#gpio-cells = <0x3>;
			interrupt-controller;
			#interrupt-cells = <0x3>;
			phandle = <0x28>;

			r-ir-rx-pin {
				pins = "PL11";
				function = "s_cir_rx";
			};

			r-i2c-pins {
				pins = "PL0", "PL1";
				function = "s_i2c";
				phandle = <0x26>;
			};
		};

		system-control@1c00000 {
			compatible = "allwinner,sun50i-h5-system-control";
			reg = <0x1c00000 0x1000>;
			#address-cells = <0x1>;
			#size-cells = <0x1>;
			ranges;
			phandle = <0x10>;

			sram@18000 {
				compatible = "mmio-sram";
				reg = <0x18000 0x1c000>;
				#address-cells = <0x1>;
				#size-cells = <0x1>;
				ranges = <0x0 0x18000 0x1c000>;

				sram-section@0 {
					compatible = "allwinner,sun50i-h5-sram-c1", "allwinner,sun4i-a10-sram-c1";
					reg = <0x0 0x1c000>;
					phandle = <0x27>;
				};
			};
		};

		video-codec@1c0e000 {
			compatible = "allwinner,sun50i-h5-video-engine";
			reg = <0x1c0e000 0x1000>;
			clocks = <0x3 0x29 0x3 0x6c 0x3 0x61>;
			clock-names = "ahb", "mod", "ram";
			resets = <0x3 0x1a>;
			interrupts = <0x0 0x3a 0x4>;
			allwinner,sram = <0x27 0x1>;
		};

		gpu@1e80000 {
			compatible = "allwinner,sun50i-h5-mali", "arm,mali-450";
			reg = <0x1e80000 0x30000>;
			interrupts = <0x0 0x60 0x4 0x0 0x61 0x4 0x0 0x63 0x4 0x0 0x64 0x4 0x0 0x65 0x4 0x0 0x66 0x4 0x0 0x67 0x4 0x0 0x68 0x4 0x0 0x69 0x4 0x0 0x6a 0x4 0x0 0x6b 0x4 0x0 0x62 0x4>;
			interrupt-names = "gp", "gpmmu", "pp", "pp0", "ppmmu0", "pp1", "ppmmu1", "pp2", "ppmmu2", "pp3", "ppmmu3", "pmu";
			clocks = <0x3 0x31 0x3 0x72>;
			clock-names = "bus", "core";
			resets = <0x3 0x23>;
			assigned-clocks = <0x3 0x72>;
			assigned-clock-rates = <0x16e36000>;
		};
	};

	cpus {
		#address-cells = <0x1>;
		#size-cells = <0x0>;

		cpu@0 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x0>;
			enable-method = "psci";
		};

		cpu@1 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x1>;
			enable-method = "psci";
		};

		cpu@2 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x2>;
			enable-method = "psci";
		};

		cpu@3 {
			compatible = "arm,cortex-a53";
			device_type = "cpu";
			reg = <0x3>;
			enable-method = "psci";
		};
	};

	psci {
		compatible = "arm,psci-0.2";
		method = "smc";
	};

	timer {
		compatible = "arm,armv8-timer";
		interrupts = <0x1 0xd 0xf08 0x1 0xe 0xf08 0x1 0xb 0xf08 0x1 0xa 0xf08>;
	};

	aliases {
		ethernet0 = "/soc/ethernet@1c30000";
		serial0 = "/soc/serial@1c28000";
		i2c0 = "/soc/i2c@1c2ac00";
	};

	leds {
		compatible = "gpio-leds";

		pwr {
			label = "nanopi:green:pwr";
			gpios = <0x28 0x0 0xa 0x0>;
			default-state = "on";
		};

		status {
			label = "nanopi:blue:status";
			gpios = <0xa 0x0 0xa 0x0>;
			linux,default-trigger = "heartbeat";
		};
	};

	gmac-3v3 {
		compatible = "regulator-fixed";
		regulator-name = "gmac-3v3";
		regulator-min-microvolt = <0x325aa0>;
		regulator-max-microvolt = <0x325aa0>;
		startup-delay-us = <0x186a0>;
		enable-active-high;
		gpio = <0xa 0x3 0x6 0x0>;
		phandle = <0x12>;
	};

	vcc3v3 {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3";
		regulator-min-microvolt = <0x325aa0>;
		regulator-max-microvolt = <0x325aa0>;
		phandle = <0x9>;
	};

	usb0-vbus {
		compatible = "regulator-fixed";
		regulator-name = "usb0-vbus";
		regulator-min-microvolt = <0x4c4b40>;
		regulator-max-microvolt = <0x4c4b40>;
		enable-active-high;
		gpio = <0x28 0x0 0x2 0x0>;
		status = "okay";
		phandle = <0xd>;
	};

	connector {
		compatible = "hdmi-connector";
		type = [61 00];

		port {

			endpoint {
				remote-endpoint = <0x29>;
				phandle = <0x23>;
			};
		};
	};

	sound_i2s {
		compatible = "simple-audio-card";
		simple-audio-card,name = "WM8978";
		simple-audio-card,mclk-fs = <0x100>;
		simple-audio-card,format = "i2s";
		simple-audio-card,bitclock-master = <0x2a>;
		simple-audio-card,frame-master = <0x2a>;
		status = "disabled";

		simple-audio-card,cpu {
			sound-dai = <0x2b>;
			phandle = <0x2a>;
		};

		simple-audio-card,codec {
			sound-dai = <0x2c>;
		};
	};

	sound_ac108 {
		compatible = "simple-audio-card";
		simple-audio-card,name = "ac108-card";
		simple-audio-card,format = "dsp_a";
		simple-audio-card,mclk-fs = <0x200>;
		simple-audio-card,bitclock-master = <0x2d>;
		simple-audio-card,frame-master = <0x2d>;
		simple-audio-card,channels-playback-override = <0x4>;
		simple-audio-card,channels-capture-override = <0x4>;
		status = "okay";

		simple-audio-card,cpu {
			sound-dai = <0x2b>;
			dai-tdm-slot-num = <0x4>;
			dai-tdm-slot-width = <0x20>;
			dai-tdm-slot-tx-mask = <0x1 0x1 0x1 0x1>;
			dai-tdm-slot-rx-mask = <0x1 0x1 0x1 0x1>;
			phandle = <0x2d>;
		};

		simple-audio-card,codec {
			sound-dai = <0x2e>;
			clocks = <0x2f>;
		};
	};
};
